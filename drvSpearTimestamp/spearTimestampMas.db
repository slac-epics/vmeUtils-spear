# records to be loaded on a MASTER only
record (bo, "Spear:Event1")
{
	field(DESC, "Spear TSSM Event 1")
	field(DTYP, "BusAddress")
	field(OUT,  "#C0 S0 @tssmMas+0x1c,tssmIO")
	field(MASK, "0x0100")
	field(VAL,  "0")
	field(PINI, "YES")
	field(ZNAM, "Inactive")
	field(ONAM, "Active")
	field(TSE,  "-2")
	field(HIGH, "0.5")
}
record(calcout, "Spear:Event1Trig")
{
	field(DESC, "Ev. Trig. Cond.+Source")
	field(OUT,  "Spear:Event1 PP")
}
